{
  "name": "ts-nextjs-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prepare": "husky install",
    "tailwind:watch": "tailwindcss -i src/styles/globals.css -o src/build.css --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.8.0-rc.2",
    "@apollo/experimental-nextjs-app-support": "^0.4.3",
    "@headlessui/react": "^1.7.17",
    "@material-tailwind/react": "^2.1.2",
    "@next/font": "^14.0.3",
    "@react-email/components": "^0.0.12",
    "@react-email/render": "^0.0.10",
    "@types/nodemailer": "^6.4.14",
    "@types/react-paginate": "^7.1.4",
    "@vercel/speed-insights": "^1.0.1",
    "axios": "^1.5.1",
    "clsx": "^1.2.1",
    "framer-motion": "^10.16.5",
    "graphql": "^16.8.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.260.0",
    "mysql2": "^3.6.1",
    "next": "^13.4.10",
    "next-usequerystate": "^1.13.0",
    "nodemailer": "^6.9.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-leaflet": "^4.2.1",
    "react-multi-carousel": "^2.8.4",
    "react-paginate": "^8.2.0",
    "react-responsive-carousel": "^3.2.23",
    "tailwind-merge": "^1.13.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@svgr/webpack": "^8.0.1",
    "@tailwindcss/forms": "^0.5.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/leaflet": "^1.9.8",
    "@types/react": "^18.2.41",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.10",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.5.0",
    "next-router-mock": "^0.7.5",
    "next-sitemap": "^2.5.28",
    "postcss": "^8.4.32",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier -w"
    ]
  }
}
